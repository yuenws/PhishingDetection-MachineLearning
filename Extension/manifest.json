{
  "manifest_version": 2,
  "name": "Phishing Checker",
  "short_name": "Search",
  "description": "Check phishing URL",
  "version": "0.0.1",
  "minimum_chrome_version": "38",
  "browser_action": {
    "default_icon": "assets/search.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus", 
    "tabs"],
  "icons": {
    "16": "assets/search.png"
  },
  "background": {
    "scripts" : ["background.js"],
    "persistent": false
  }
}
